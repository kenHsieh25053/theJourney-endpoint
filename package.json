{
  "name": "the-journey-app",
  "version": "1.0.0",
  "description": "an app which allows user to note, plan, share they journey",
  "main": "index.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon --exec babel-node src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "verify": "./node_modules/.bin/eslint ./src/",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx}\"",
    "init": "node_modules/.bin/sequelize init",
    "migrate": "node_modules/.bin/sequelize db:migrate --env development",
    "undo": "node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "seed": "node_modules/.bin/sequelize db:seed:all --env development",
    "dbsetup": "npm run undo && npm run migrate && npm run seed"
  },
  "keywords": [
    "travel",
    "journey",
    "the journey"
  ],
  "author": "Ken Hsieh",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.0.0",
    "amazon-s3-uri": "0.0.3",
    "apollo-server-cache-memcached": "^0.6.4",
    "apollo-server-express": "^2.9.9",
    "aws-sdk": "^2.94.0",
    "babel-core": "^7.0.0-bridge.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.18.2",
    "compression": "^1.7.4",
    "cors": "^2.8.4",
    "dotenv": "^8.0.0",
    "express": "^4.16.2",
    "googleapis": "^20.1.0",
    "graphql": "^14.5.8",
    "graphql-depth-limit": "^1.1.0",
    "graphql-tools": "^4.0.6",
    "graphql-upload": "^8.0.4",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^7.4.3",
    "jwks-rsa": "^1.6.0",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.3",
    "mysql2": "^2.0.0",
    "robots-parser": "^2.1.1",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.0",
    "stream-to-buffer": "^0.1.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^10.2.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.19.1"
  }
}
